<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebBrowser13._05._2019"
        xmlns:Forms="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms" x:Class="WebBrowser13._05._2019.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="415" Margin="0,4,0,0" VerticalAlignment="Top" Width="787">
            <TabItem Name="tabItem" Header="Google" Margin="-2,-2,3,0">
                <WebBrowser x:Name="webbrowser" Margin="0,0,-6,0"/>
            </TabItem>
            <TabItem MouseLeftButtonUp="tabItemPlus_MouseLeftButtonUp" Name="tabItemPlus" Header="+" Margin="-2,-2,3,0">
            </TabItem>



        </TabControl>

    </Grid>
</Window>
